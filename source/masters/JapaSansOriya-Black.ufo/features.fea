# automatic
languagesystem DFLT dflt;
languagesystem ory2 dflt;
feature aalt {
    # automatic
    feature akhn;
    feature nukt;
    feature rphf;
    feature blwf;
    feature pstf;
    feature cjct;
    feature pres;
    feature blws;
    feature psts;
    feature abvs;
    feature haln;
    feature calt;
    feature ss01;
    feature ss02;
    feature ss03;
} aalt;

feature akhn {
    script ory2;
    sub ka-oriya halant-oriya ssa-oriya by kassa-oriya;
    sub ja-oriya halant-oriya nya-oriya by janya-oriya;
} akhn;

